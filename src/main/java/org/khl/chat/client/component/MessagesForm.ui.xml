<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:c='urn:import:com.google.gwt.user.cellview.client'
  xmlns:s="urn:import:org.khl.chat.client.component">

  <ui:style>
    .scrollable {
      height: 400px;
      width: 550px;
      border: 1px solid #ccc;
      text-align: left;
    }
    
    .contactFormCell {
      padding-left: 20px;
    }
  </ui:style>

  <g:HTMLPanel>
    <table>
      <tr>
        <td
          valign='top'
          align='center'>
          <s:RangeLabelPager
            ui:field='rangeLabelPager' />
          <s:ShowMorePagerPanel
            addStyleNames='{style.scrollable}'
            ui:field='pagerPanel' />
        </td>
      </tr>
      <tr>
        <td
          align='center'>
          <s:ShowMorePagerPanel
            ui:field='pager' />
        </td>
      </tr>
    </table>
  </g:HTMLPanel>
</ui:UiBinder>