<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<g:AbsolutePanel>
    	<g:Label text="Name"></g:Label>
    	<g:TextBox ui:field="tbName"></g:TextBox>
    	<g:Label text="Email"></g:Label>
    	<g:TextBox ui:field="tbEmail"></g:TextBox>
    	<g:Label text="Password"></g:Label>
    	<g:PasswordTextBox ui:field="tbPassword"></g:PasswordTextBox>
    	<g:Button ui:field="btnSignUp" text="Зарегистрироваться"></g:Button>
    	<g:Button ui:field="btnSignIn" text="Авторизация"></g:Button>
	</g:AbsolutePanel>
	
</ui:UiBinder>